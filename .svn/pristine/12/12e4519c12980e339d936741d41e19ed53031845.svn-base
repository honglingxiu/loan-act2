}
.theme1 .toolbar {
    color: #499bed;
}
.theme1 .mint-indexsection-index {
    color: #499bed;
}
.theme1 .mint-indexlist-navitem {
    color: #499bed;
}
.theme1 .mint-datetime-confirm {
    color: #499bed;
}
.theme1 .mint-popup .additional {
    color: #499bed;
}
.theme1 .popup-header {
    background: url(img/alertbg1.png) no-repeat 0 0/100% 100%;
}
.theme1 .extra-info-tip {
