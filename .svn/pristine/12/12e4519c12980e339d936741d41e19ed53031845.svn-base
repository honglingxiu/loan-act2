    right: -0.09rem;
    width: .18rem;
    height: .18rem;
    background: url(img/check.png) no-repeat 0 0/100% 100%;
}
.theme1 .loan-time-radio:checked + .radio-btn {
    background: #499bed;
}
.theme1 .popup-title {
    color: #60728b
}
